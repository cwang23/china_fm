function search_checkeng() {
var sword =document.getElementById("sw").value.replace(/^\s+|\s+$/g,"");
	
if (sword=='Please enter keywords' || sword=="") {
	alert('Please enter keywords!');
	return false;
} else {	
	return true;
	}
}
</script>
